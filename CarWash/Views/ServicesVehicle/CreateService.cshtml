@model CarWash.Models.AddServicesVehicleViewModel
@{
    ViewData["Title"] = "CreateService";
}

<h1>CreateService</h1>
<hr />
<div class="row">
    <div class="col-md-12">
        <form asp-action="CreateService" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>

            <partial name="../Shared/PartialViews/_Create_ServicesVehicle" />

            <div class="form-group mt-2">
                <input type="submit" value="Create" class="btn btn-outline-primary" />
                <a asp-action="Index" class="btn btn-outline-success">Back</a>
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}